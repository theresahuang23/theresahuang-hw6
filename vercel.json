{
  "version": 2,
  "buildCommand": "echo 'Python app - no build needed'",
  "builds": [
    {
      "src": "company_formation_server/app.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb",
        "runtime": "python3.11",
        "requirements": "company_formation_server/requirements.txt"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "company_formation_server/app.py"
    }
  ]
}
